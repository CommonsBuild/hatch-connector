{
  "name": "@tecommons/subgraph-hatch",
  "version": "0.2.0",
  "license": "GPL-3.0",
  "description": "Hatch subgraph used by the connector",
  "author": "TECommons",
  "scripts": {
    "codegen": "rm -rf generated; graph codegen",
    "build": "rm -rf build; graph build",
    "manifest-mainnet": "scripts/build-manifest.sh mainnet",
    "manifest-mainnet-staging": "env STAGING=true scripts/build-manifest.sh mainnet",
    "manifest-rinkeby": "scripts/build-manifest.sh rinkeby",
    "manifest-rinkeby-staging": "env STAGING=true scripts/build-manifest.sh rinkeby",
    "manifest-xdai": "scripts/build-manifest.sh xdai",
    "deploy-mainnet": "scripts/deploy.sh tecommons hatch mainnet",
    "deploy-mainnet-staging": "env STAGING=true scripts/deploy.sh tecommons presale mainnet",
    "deploy-rinkeby": "scripts/deploy.sh tecommons hatch rinkeby",
    "deploy-rinkeby-staging": "env STAGING=true scripts/deploy.sh tecommons hatch rinkeby",
    "deploy-xdai": "scripts/deploy.sh tecommons hatch xdai"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.1",
    "mustache": "^4.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
